<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <!-- Gestion de permisos -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".Administrador.Funciones"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_doc_app"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_doc_app_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DocumentManager"
        tools:targetApi="31">
        <activity
            android:name=".Administrador.EditarPerfilAdmin"
            android:exported="false" />
        <activity
            android:name=".LeerDocumento"
            android:exported="false" />
        <activity
            android:name=".Administrador.DetalleDocumento"
            android:exported="false" />
        <activity
            android:name=".Administrador.ActualizarDocumento"
            android:exported="false" />
        <activity
            android:name=".Administrador.ListaDocumentoAdmin"
            android:exported="false" />
        <activity
            android:name=".Administrador.Agregar_Documento"
            android:exported="false" />
        <activity
            android:name=".Administrador.Agregar_Categoria"
            android:exported="false" />
        <activity
            android:name=".Administrador.Login_Admin"
            android:exported="false" />
        <activity
            android:name=".Administrador.Registrar_Admin"
            android:exported="false" />
        <activity
            android:name=".Elegir_rol"
            android:exported="false" />
        <activity
            android:name=".Bienvenida"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>